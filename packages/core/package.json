{
  "name": "@w3filecoin/core",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "mock:spade-oracle-server": "node test/helpers/spade-oracle-server.js",
    "mock": "run-p mock:*",
    "test": "PORT=9200 npm-run-all -p -r mock test:all",
    "test:all": "ava --serial --no-worker-threads --verbose --timeout=120s test/{*.test.js,**/*.test.js}"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "catalog:",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/client-sqs": "catalog:",
    "@aws-sdk/util-dynamodb": "catalog:",
    "@ipld/dag-json": "catalog:",
    "@ipld/dag-ucan": "catalog:",
    "@serverless-stack/node": "^1.18.4",
    "@storacha/capabilities": "catalog:",
    "@storacha/filecoin-api": "catalog:",
    "@storacha/filecoin-client": "catalog:",
    "@ucanto/client": "catalog:",
    "@ucanto/interface": "catalog:",
    "@ucanto/principal": "catalog:",
    "@ucanto/server": "catalog:",
    "@ucanto/transport": "catalog:",
    "@web3-storage/data-segment": "catalog:",
    "fzstd": "catalog:",
    "multiformats": "catalog:",
    "p-all": "catalog:",
    "p-retry": "catalog:",
    "pretty-ms": "catalog:",
    "stream-read-all": "catalog:",
    "uint8arrays": "catalog:"
  },
  "devDependencies": {
    "@ipld/car":"catalog:",
    "@web-std/blob": "catalog:",
    "ava": "catalog:",
    "delay": "catalog:",
    "nanoid": "catalog:",
    "npm-run-all": "catalog:",
    "p-defer": "catalog:",
    "p-wait-for": "catalog:",
    "sqs-consumer": "catalog:",
    "testcontainers": "catalog:"
  },
  "engines": {
    "node": ">=18.16"
  },
  "eslintConfig": {
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/no-for-loop": "off",
      "unicorn/no-null": "off",
      "unicorn/no-useless-promise-resolve-reject": "off",
      "unicorn/prefer-math-trunc": "off",
      "jsdoc/require-param": "off"
    }
  }
}
